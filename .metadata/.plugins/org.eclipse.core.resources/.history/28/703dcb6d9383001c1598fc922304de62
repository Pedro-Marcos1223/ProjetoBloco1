package Quizz2;

import java.util.Scanner;

public class ClasseMae {

	public static void main(String [] args) {
		
		Scanner ler = new Scanner(System.in);
		
		String x;
		
		System.out.println("Digite uma letra: ");
		x=ler.next();
		
		if(x=="a" || x=="A") {
			System.out.println("Certo");
		} else {
			System.out.println("Errado");
		}
		
		
		
		
		
		
		
		
		
	}
	
	
	
	
	
	
	
	
	
	
	
	
}
