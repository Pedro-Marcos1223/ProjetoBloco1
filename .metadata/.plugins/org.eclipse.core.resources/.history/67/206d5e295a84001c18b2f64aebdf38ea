package Herança1;

public class Principal {

	public static void main(String[] args) {
	
		Cachorro cachorro = new Cachorro();
		
		cachorro.setNome("Bolota");
		cachorro.setIdade(11);
		cachorro.setPeso(12.0);
		cachorro.status();
		cachorro.darAPata();
		cachorro.emitirSom();
		cachorro.dormindo();
		
		
		
	}

}
