
public class testaVariaveis {

	public static void main(String[] args) {
	
		System.out.println("aprendendo a aprender");
		
		int idade;
		idade = 19;
		System.out.println(idade);
		
		idade = 19 * 5 * 10;
		System.out.println(idade);
		
		idade = 5 + 10;
		System.out.println(idade);
		
		System.out.println("a idade é: " + idade);
	
	}
	
	
	
	
	
	
	
	
	
	
	
}
