package Heranca;

public class ObjetoAnimais {

	public static void main(String [] args) {
		
		Cachorro p1 = new Cachorro();
		Cavalo p2 = new Cavalo();
		Preguica p3 = new Preguica();
		
		
		p1.setNome("Bolota");
